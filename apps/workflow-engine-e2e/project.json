{
  "name": "workflow-engine-e2e",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "implicitDependencies": ["workflow-engine"],
  "targets": {
    "e2e": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
      "options": {
        "jestConfig": "workflow-engine-e2e/jest.config.ts",
        "passWithNoTests": true
      },
      "dependsOn": ["workflow-engine:build", "workflow-engine:serve"]
    }
  }
}
