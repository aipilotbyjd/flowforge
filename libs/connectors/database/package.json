{
  "name": "@flowforge/connectors-database",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "nx": {
    "name": "connectors-database",
    "tags": [
      "scope:connectors",
      "type:database"
    ],
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "dist/libs/connectors/database",
          "tsConfig": "libs/connectors/database/tsconfig.lib.json",
          "packageJson": "libs/connectors/database/package.json",
          "main": "libs/connectors/database/src/index.ts",
          "assets": [
            "libs/connectors/database/*.md"
          ]
        }
      }
    }
  },
  "dependencies": {
    "tslib": "^2.3.0"
  }
}
